{
    "type": {
        "$id": "type",
        "description": "System type",
        "value": "AHU"
    },
    "subtype": {
        "$id": "subtype",
        "description": "Type of AHU",
        "widget": {
            "type": "Dropdown",
            "options": ["VAV", "Dual Fan Dual Duct", "DOA", "Supply only", "Exhaust only"]
        },
        "value": "VAV"
    },
    "name": {
        "$id": "name",
        "description": "Model name",
        "widget": {
            "type": "Text input"
        }
    },
    "equipment": [
        {
            "$id": "eco",
            "description": "Economizer",
            "enabled": "#subtype.value == 'VAV'",
            "widget": {
                "type": "Dropdown menu",
                "options": ["None", "Common OA damper", "Dedicated OA damper"]
            },
            "value": "None",
            "declaration": [
                null,
                "eco",
                "eco_cla1"
            ]
        },
        {
            "$id": "fanSupTyp",
            "description": "Type",
            "group": "Supply fan",
            "enabled": "#subtype.value != 'Exhaust only'",
            "widget": {
                "type": "Dropdown menu",
                "options": ["None", "Single", "Array"]
            },
            "value": "Single",
            "declaration": [
                null,
                "fanSup",
                "fanSup_cla1"
            ]
        },
        {
            "$id": "fanSupPos",
            "description": "Position",
            "group": "Supply fan",
            "enabled": "#fanSupTyp.value != 'None'",
            "widget": {
                "type": "Dropdown menu",
                "options": ["None", "Draw through", "Blow through"]
            },
            "value": "Draw through",
            "declaration": [
                null,
                "fanSup",
                "fanSup_pos1"
            ]
        }
    ],
    "controls": [
        {
            "$id": "have_airFloMeaSta",
            "description": "Set to true if the AHU has Air Flow Measurement Station",
            "enabled": "#eco != 'None'",
            "widget": {
                "type": "Boolean select"
            },
            "value": false,
            "declaration": "have_airFloMeaSta"
        }
    ]
}
